{
  "name": "app",
  "description": "An awesome app!",
  "version": "0.0.1",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . app --platform win32 --arch x64 --out dist/",
    "setup": "electron-installer-windows --src release-builds/app-win32-x64/ --dest dist/installers/"
  },
  "devDependencies": {
    "electron-installer-windows": "^1.1.0",
    "electron-packager": "*",
    "electron-prebuilt": "*",
    "electron-winstaller": "^2.6.4"
  }
}
